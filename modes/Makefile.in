PIXMAPDIR = @PIXMAPDIR@/
UTILDIR = $(top_srcdir)/xlock/
GLDIR = $(top_srcdir)/modes/glx/
UTILOBJDIR = ../xlock/
GLOBJDIR = glx/

N=
O=.@OBJEXT@
#O=.obj
C=.c
CX=.cc
S=$(N) $(N)
