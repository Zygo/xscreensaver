XScreenSaver_ad.h::
	@TMP=/tmp/xs$$$$.h ;						\
	IN="XScreenSaver.ad" ;						\
	OUT=XScreenSaver_ad.h ;						\
	$(UTILS_SRC)/ad2c $$IN > $$TMP ;				\
	if cmp -s $$TMP $$OUT ; then					\
	  rm -f "$$TMP" ;						\
	else								\
	  echo $(UTILS_SRC)/ad2c $$IN \> $$OUT ;			\
	  mv $$TMP $$OUT ;						\
	fi

