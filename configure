# Print some warnings before emitting the Makefiles.
#
warning=no
warnsep='    #################################################################'
warnpre() {
  if test $warning = no ; then
    echo '' ; echo "$warnsep" ; echo ''
    warning=yes
  fi
}

warn() {
  warnpre
  if test $warning = long ; then echo '' ; fi
  warning=yes
  echo "    Warning: $@"
}

warnL() {
  was=$warning
  warnpre
  warning=yes
  if test $was != no ; then echo '' ; fi
  echo "    Warning: $@"
}

warn2() {
  echo "             $@"
  warning=long
}

